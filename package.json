{
  "name": "todoapp",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://nextentrepreneur.github.com/todo/",
  "dependencies": {
    "@material-ui/core": "^3.6.1",
    "concurrently": "^4.1.0",
    "dialog": "^0.3.1",
    "electron-gh-releases": "^2.0.4",
    "electron-gh-releases-updater": "^0.8.0",
    "electron-is-dev": "^1.0.1",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.6",
    "firebase": "^5.7.0",
    "gh-pages": "^2.0.1",
    "grunt": "^1.0.3",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "^6.0.0",
    "react-scripts": "2.1.1",
    "redux": "^4.0.1",
    "typeface-roboto": "0.0.54",
    "wait-on": "^3.2.0"
  },
  "build": {
    "extends": null
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nextentrepreneur/todo.git"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "eject": "react-scripts eject",
    "electron": " electron .",
    "publish": "node_modules/.bin/build --win -p always",
    "electron-dev": "concurrently \"SET BROWSER=none&&npm run start\" \"wait-on http://localhost:3000 && electron .\""
  },
  "nsis": {
    "perMachine": true
  },
  "main": "public/main.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "description": "This application is built with Electron, React, Redux, and FireBase.",
  "devDependencies": {
    "electron": "^3.0.10",
    "electron-builder": "^20.38.2",
    "foreman": "^3.0.1"
  },
  "files": [
    "*.js",
    "build",
    "node_modules"
  ],
  "author": "Sujit Roy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nextentrepreneur/todo/issues"
  }
}
